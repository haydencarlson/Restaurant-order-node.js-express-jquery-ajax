<!DOCTYPE html>
	<html lang="en">
		<head>
		  <title>Admin Order Page</title>

			<link rel="stylesheet" href="/css/admin.css" type="text/css" />
			<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" >
			<script
				src="https://code.jquery.com/jquery-3.1.1.min.js"
				integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
				crossorigin="anonymous">
			</script>
			<link rel="stylesheet" href="/scripts/remodal.css">
			<link rel="stylesheet" href="/scripts/remodal-default-theme.css">
			<script src="/scripts/remodal.min.js"></script>
		</head>

		<body>
		<nav id="adminNav">
			<li><a id="addItem" href="#modal">Add Menu Item</a></li>	
		</nav>

		<div class="remodal-bg">
			<div class="remodal" data-remodal-id="modal">
			  <button data-remodal-action="close" class="remodal-close"></button>
			  <h1>Add New Menu Item</h1>
			  <p>
			    What would you like to add?
			  </p>
			  <br>
			  <button data-remodal-action="cancel" class="remodal-cancel">Cancel</button>
			  <button data-remodal-action="confirm" class="remodal-confirm">OK</button>
			</div>
		</div>

			<% for(var keys in orderID) { %>


					<div  class='container ready' name="newOrder">
						<div class="row">
							<div class='col-xs-12'>

								<!-- Insert order_id -->
					  		<h3 class= 'col-xs-12' id="orderID"><%= "Order ID: " + keys %></h3>

								<div class='col-xs-12'>

									<!-- Insert the array of order_items -->
									<%	orderID[keys].forEach(function (el) { %>
									<li class='fooditem'><%= el %></li>
									<% }) %>

								</div>

								<button class="btn btn-danger ready confirm" >Order Ready</button>

							</div>
						</div>
					</div>
			<%  } %>

			<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
			<script type="text/javascript" src="./script.js"></script>
			<script type="text/javascript" src="/testfolder/adminBs	.js"></script>
		</body>
	</html>



















